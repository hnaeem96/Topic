<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <div id="upper-center">
      <div id="topic">
        <h3>Topic</h3>
      </div>
    </div>

    <div class="center-center">
      <div id="search-bar">
        <%= form_tag("/topic") do %>
          <%= label_tag("#") %>
          <%= text_field_tag('topic') %>
          <%= submit_tag "Search" %>
        <% end %>
      </div>
    </div>
  </body>
</html>
